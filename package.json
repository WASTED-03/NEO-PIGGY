{
  "name": "nextjs-base-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint & npm run type-check & wait"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.2.23",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "supabase": "2.34.3",
    "tailwindcss": "^3.4.1",
    "oxlint": "^1.12.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "supabase",
      "unrs-resolver"
    ],
    "peerDependencyRules": {
      "allowAny": [
        "supabase"
      ]
    },
    "patchedDependencies": {
      "supabase@2.34.3": ".easycode-ai/supabase/v2.34.3/supabase@2.34.3.patch"
    }
  }
}
